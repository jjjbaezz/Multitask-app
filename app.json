{
  "expo": {
    "name": "Multitask-App",
    "slug": "Multitask-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Mi-foto.png",
    "scheme": "multitaskapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "WEATHER_API_KEY": "",
      "router": {},
      "eas": {
        "projectId": "d82df22d-dd7d-44aa-8611-4c63b125059a"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jjjbaezz.multitaskapp",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/Mi-foto.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "jjjbaezz"
  }
}
